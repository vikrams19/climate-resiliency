{
  "projects": {
    "rootDirectory": "frontend"
  },
  "builds": [
    {
      "src": "frontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "frontend/build"
      }
    }
  ],
  "rewrites": [
    { "source": "/(.*)", "destination": "/frontend/$1" }
  ]
}
